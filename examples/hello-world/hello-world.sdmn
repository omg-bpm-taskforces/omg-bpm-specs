<?xml version="1.0" encoding="UTF-8"?>
<sdmn:SDMNModelPackage id="HelloWorldDataModel"
    targetNamespace="https://example.org/hello-world/shared-data-model"
    xmlns="https://example.org/hello-world/shared-data-model"
    xmlns:sdmn="https://www.omg.org/spec/SDMN/20211108/MODEL"
    xmlns:sdmndi="https://www.omg.org/spec/SDMN/20211108/SDMNDI"
    xmlns:multiplicity="https://www.omg.org/spec/SCE/20211108/MODEL/SDMNLibrary/MultiplicityKinds"
    xmlns:itemkind="https://www.omg.org/spec/SCE/20211108/MODEL/SDMNLibrary/ItemKinds"
    xmlns:sce="https://www.omg.org/spec/SCE/20211108/MODEL"
    xmlns:dmn="https://www.omg.org/spec/DMN/20191111/MODEL/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    >

  <!-- FIXME: overriding of model & presentation instead of duplicating them -->
  <sce:model id="fixme" targetNamespace="fixme:SCE-41" />
  <sce:presentation/>
  <sdmn:SDMNModel id="model1" targetNamespace="fixme:SCE-41">
    <sdmn:SDMNDefinitions targetNamespace="fixme:SCE-41" id="definitions1">

      <sdmn:dataItem id="dataItem_Name" multiplicityKind="Unspecified">
        <sdmn:typeDefinitionRef>itemDef_Name</sdmn:typeDefinitionRef>
      </sdmn:dataItem>
      <sdmn:dataItem id="dataItem_Greeting"/>

      <!-- DMN will reference by @name while BPMN uses @id -->
      <sdmn:itemDefinition id="itemDef_Name" name="Name" itemKind="DataType" multiplicityKind="Unspecified" isCollection="false">
        <dmn:typeRef>string</dmn:typeRef>
      </sdmn:itemDefinition>
    </sdmn:SDMNDefinitions>
  </sdmn:SDMNModel>
  <sdmndi:SDMNDI></sdmndi:SDMNDI>
</sdmn:SDMNModelPackage>